<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:wp='urn:import:edu.stanford.bmir.protege.web.client.primitive'
             xmlns:button="urn:import:edu.stanford.bmir.protege.web.client.ui.library.button">
    <g:HTMLPanel>
        <table width="100%">
            <col width="30%"/>
            <tr>
                <td>
                    <wp:DefaultPrimitiveDataEditor ui:field="propertyEditor"/>
                </td>
                <td>
                    <wp:DefaultPrimitiveDataEditor ui:field="valueEditor" classesAllowed="true"/>
                </td>
                <td width="30px">
                    <wp:DefaultLanguageEditor ui:field="languageEditor"/>
                </td>
                <td width="20px">
                    <button:DeleteButton/>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>