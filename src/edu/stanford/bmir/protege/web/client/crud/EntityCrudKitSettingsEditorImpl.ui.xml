<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel>
        <table>
            <colgroup>
                <col width="100px"/>
            </colgroup>
            <tr>
                <td>
                    <g:Label text="IRI Prefix" addStyleNames="web-protege-form-label"/>
                </td>
                <td>
                    <g:TextBox ui:field="iriPrefixEditor" visibleLength="60"/>
                </td>
            </tr>
            <tr>
                <td><g:Label text="IRI Suffix" addStyleNames="web-protege-form-label"/></td>
                <td><g:ListBox ui:field="schemeSelectorListBox" visibleItemCount="1"/></td>
            </tr>
        </table>
        <div style="height: 10px;"/>
        <g:SimplePanel ui:field="schemeSpecificSettingsEditorHolder"/>
        <div style="height: 10px; border-bottom: 1px solid #f1f1f1;"/>
        <div style="height: 10px;"/>
        <table>
            <colgroup>
                <col width="100px"/>
            </colgroup>
            <tr>
                <td>
                    <g:Label text="Example IRI" addStyleNames="web-protege-form-label"/>
                </td>
                <td>
                    <g:Label ui:field="exampleField"/>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>