<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:wp='urn:import:edu.stanford.bmir.protege.web.client.ui.frame'>

    <ui:style src="Frame.css"/>


    <g:HTMLPanel styleName="web-protege-laf" addStyleNames="{style.main}">

        <div class="{style.form-group}">
            <g:Label text="Display name" addStyleNames="{style.form-label}"/>
            <g:TextBox ui:field="displayNameField" width="100%"/>
        </div>

        <div class="{style.form-group}">
            <g:Label text="IRI" addStyleNames="{style.form-label}"/>
            <g:TextBox ui:field="iriField" width="100%"/>
        </div>


        <div style="height: 30px;"/>

        <g:Label text="Annotations" addStyleNames="{style.form-label}"/>
        <wp:PropertyValueListEditor ui:field="annotations"/>

        <div style="height: 30px;"/>

        <g:Label text="Properties" addStyleNames="{style.form-label}"/>
        <wp:PropertyValueListEditor ui:field="properties"/>

    </g:HTMLPanel>
</ui:UiBinder>
