<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .main {
            width: 500px;
            margin-left: auto;
            margin-right: auto;
            text-align: center;
            line-height: 40px;
            font-size: 16px;
            margin-top: 30px;
            color: #606060;
            font-weight: 200;
        }
    </ui:style>

    <g:HTMLPanel addStyleNames="{style.main}">
        <div>
            Could not create view.
        </div>
        <div>
            The view specified in the configuration could not be found.
        </div>
    </g:HTMLPanel>
</ui:UiBinder>