<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
  
<module rename-to="webprotege">

    <source pathSupplier="client"/>
    <source pathSupplier="shared"/>
    <source pathSupplier="resources"/>


    <inherits nameSupplier='com.google.gwt.junit.JUnit'/>

    <inherits nameSupplier="com.google.gwt.user.User"/>
    <inherits nameSupplier="com.google.gwt.resources.Resources" />
    <inherits nameSupplier="com.google.gwt.place.Place"/>
    <inherits nameSupplier="com.google.common.cache.Cache"/>
    <inherits nameSupplier="com.google.common.collect.Collect"/>
    <inherits nameSupplier="com.google.common.base.Base"/>
    <inherits nameSupplier="com.google.gwt.http.HTTP"/>
    <inherits nameSupplier="com.google.gwt.json.JSON"/>
    <inherits nameSupplier="com.google.common.io.Io"/>
    <inherits nameSupplier="edu.stanford.bmir.gwtcodemirror.CodeMirror"/>

    <inherits nameSupplier="com.google.gwt.logging.Logging"/>

	<inherits nameSupplier='com.googlecode.gwt.crypto.Crypto'/>

    <inherits nameSupplier="org.semanticweb.owlapi.gwt.owlapi"/>

	<inherits nameSupplier='com.gwtext.GwtExt' />

    <inherits nameSupplier="edu.stanford.protege.widgetmap.widgetmap"/>
    <inherits nameSupplier="com.google.gwt.activity.Activity"/>
    <inherits nameSupplier="com.google.gwt.i18n.I18N"/>

    <inherits nameSupplier="dagger.Dagger"/>

    <extend-property nameSupplier="locale" values="de" />

    <extend-configuration-property nameSupplier="rpc.blacklist" value="+com.google.web.bindery.event.shared.Event"/>

	<entry-point class="edu.stanford.bmir.protege.web.client.WebProtege"/>

</module>
